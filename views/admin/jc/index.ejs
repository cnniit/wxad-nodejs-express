<%- include ../public/header.ejs%>
<script src="/bootstrap/js/jquery-1.10.1.js"></script>
<link rel="stylesheet" href="/bootstrap/css/bootstrap.css" />
<script src="/bootstrap/js/bootstrap.min.js"></script>


<div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">

            <%- include ../public/aslideleft.ejs%>
          
        </div>
        <div class="col-sm-10">
            <ol class="breadcrumb">
                <li class="active">广告管理
                </li>
                <li class="active"><%=list%>
                </li>
            </ol>
            <div class="panel panel-default">
                <ul id="myTab" class="nav nav-tabs">
                    <li class="active">
                        <a href="#home" data-toggle="tab">
                             单个
                        </a>
                    </li>
                    <li><a href="#s2" data-toggle="tab">多个</a></li>
                </ul>
                <div id="myTabContent" class="tab-content">
                    <div class="tab-pane fade in active" id="home">
                        <p></p>
                        <div class="panel-body">
                            <form action="/admin/jc/singlecr" method="post" role="form" class="form-inline">
                                <div class="form-group">
                                    <label for="name">表名</label>
                                    <input type="text" class="form-control" id="name" name="table_name" placeholder="请输入名称">
                                </div>
        
                                <div class="form-group">
                                    <button type="submit" class="btn btn-default">生成</button>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="s2">
                        <p></p>
                        <div class="panel-body">
                            <form action="/admin/jc/multicr" method="post" role="form" class="form-inline">
                                <div class="form-group">
                                    <label for="name">前缀</label>
                                    <input type="text" class="form-control" id="name" name="prefix_name" placeholder="请输入名称">
                                </div>
                                <div class="form-group">
                                    <label for="name">开始序号</label>
                                    <input type="text" class="form-control" id="name" name="fromindex" placeholder="请输入名称">
                                </div>
                                <div class="form-group">
                                    <label for="name">结束序号</label>
                                    <input type="text" class="form-control" id="name" name="toindex" placeholder="请输入名称">
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-default">生成</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

            </div>
            <!--
                列表展示
            -->
            <div class="table-responsive">
            </div>

        </div>
    </div>
</div>
</body>
</html>